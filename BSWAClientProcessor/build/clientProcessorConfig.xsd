<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
	targetNamespace="http://configuration/jaxb/clientProcessorConfig"
	xmlns="http://configuration/jaxb/clientProcessorConfig" xmlns:xjc="http://java.sun.com/xml/ns/jaxb/xjc"
	jaxb:extensionBindingPrefixes="xjc" xmlns:jaxb="http://java.sun.com/xml/ns/jaxb"
	jaxb:version="2.1" elementFormDefault="qualified">
				
	<xs:element name="ClientProcessorConfig">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="serverAdress" type="xs:string" />
				<xs:element name="connectionPort" type="xs:int" />
				<xs:element name="uiMode" type="xs:boolean" />
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	

</xs:schema>